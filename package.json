{
  "name": "@omnisync/source",
  "version": "1.0.0",
  "description": "Infraestructura neural para orquestación de comunicaciones e IA generativa integrada a ERP.",
  "author": "Raz Podestá - MetaShark Tech / Florianópolis-SC - Brasil",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/razpodesta/omnisync-ai-ecosystem.git"
  },
  "scripts": {
    "// --- ORQUESTACIÓN PRINCIPAL ---": "",
    "dev": "pnpm run infra:prep && nx run-many -t serve --all",
    "build": "pnpm run infra:prep && nx build admin-dashboard --prod && nx build orchestrator-api --prod",
    "start": "node dist/apps/orchestrator-api/main.js",
    "// --- CAPA DE INFRAESTRUCTURA (HIDRATACIÓN) ---": "",
    "infra:prep": "pnpm run internationalization:build && pnpm run persistence:generate",
    "system:hydrate": "pnpm run infra:prep && pnpm run persistence:synchronize",
    "internationalization:build": "ts-node -r tsconfig-paths/register -P libs/tools/internal-scripts/tsconfig.lib.json libs/tools/internal-scripts/src/lib/run-i18n-build.ts",
    "persistence:generate": "prisma generate --schema=libs/core/persistence/prisma/schema.prisma",
    "persistence:synchronize": "prisma db push --schema=libs/core/persistence/prisma/schema.prisma",
    "// --- AUDITORÍA Y HERRAMIENTAS ---": "",
    "integrity:guardian": "ts-node -r tsconfig-paths/register -P libs/tools/internal-scripts/tsconfig.lib.json libs/tools/internal-scripts/src/lib/run-guardian.ts",
    "infrastructure:check": "ts-node -P libs/tools/internal-scripts/tsconfig.lib.json libs/tools/internal-scripts/src/lib/run-connectivity.ts",
    "infrastructure:configuration": "ts-node -P libs/tools/internal-scripts/tsconfig.lib.json libs/tools/internal-scripts/src/lib/run-config-audit.ts",
    "infrastructure:dump": "ts-node -P libs/tools/internal-scripts/tsconfig.lib.json libs/tools/internal-scripts/src/lib/run-data-snapshot.ts",
    "infrastructure:all": "pnpm run infrastructure:check && pnpm run infrastructure:configuration && pnpm run infrastructure:dump",
    "// --- CALIDAD Y ASEGURAMIENTO (QA) ---": "",
    "quality:test": "nx run-many -t test --all --parallel=3",
    "quality:lint": "nx run-many -t lint --all --parallel=3",
    "quality:format": "prettier --write .",
    "quality:types": "tsc -p tsconfig.base.json --noEmit",
    "audit": "pnpm audit && pnpm run quality:lint && pnpm run quality:types"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "pnpm": {
    "overrides": {
      "@swc/helpers": "0.5.17",
      "lodash": "^4.17.23",
      "hono@<4.11.7": ">=4.11.7",
      "next@>=15.6.0-canary.0 <16.1.5": ">=16.1.5",
      "next@>=16.0.0-beta.0 <16.1.5": ">=16.1.5",
      "next@>=16.1.0-canary.0 <16.1.5": ">=16.1.5"
