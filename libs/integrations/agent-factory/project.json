{
  "name": "agent-factory",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/integrations/agent-factory/src",
  "projectType": "library",
  "tags": ["scope:integrations", "type:application"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/integrations/agent-factory",
        "main": "libs/integrations/agent-factory/src/index.ts",
        "tsConfig": "libs/integrations/agent-factory/tsconfig.lib.json",
        "assets": [
          "libs/integrations/agent-factory/*.md",
          {
            "input": "libs/integrations/agent-factory",
            "glob": "package.json",
            "output": "."
          }
        ]
      }
    },
    "lint": { "executor": "@nx/eslint:lint" },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/integrations/agent-factory/jest.config.cts",
        "passWithNoTests": true
      }
    }
  }
}